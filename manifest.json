{
    "name": "tab-title-setter",
    "version": "0.1",
    "description": "set tab titles",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "./popup/popup.html",
        "default_icon": "./img/icon.png"
    },
    "icons": {
        "16": "./img/icon.png",
        "48": "./img/icon.png",
        "128": "./img/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}